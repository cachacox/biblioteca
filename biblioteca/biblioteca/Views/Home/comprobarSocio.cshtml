@model biblioteca.Models.socio

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>comprobarSocio</title>
</head>
<body>
    @using (Html.BeginForm())
    {
        <div class="container">
            @Html.ValidationSummary()
            <div class="col-md-4">
                <form method="post">
                    <label for="exampleInputEmail1">Busqueda Socio</label>
                    @Html.TextBoxFor(x => x.idsocio, new { @class = "form-control" })
                    <input style="display:block;margin:0 auto" name="valor" type="submit" value="Buscar" />
                </form>
            </div>            
        </div>
    }
</body>
</html>
